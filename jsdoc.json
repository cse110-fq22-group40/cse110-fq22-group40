{
    "plugins": ["plugins/markdown"],
    "recurseDepth": 10,
    "source": {
        "include": ["source/local/utils.js", "source/local/classes.js"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|jsdoc)"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    }, 
    "opts": {
        "destination": "./docs",
        "recurse": true,
        "tutorials": "./tutorials"
    }
}